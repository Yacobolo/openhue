{
  "name": "material-theme-cli",
  "version": "1.0.0",
  "description": "CLI tool that generates Material Design 3 color themes from a seed color",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "material-theme-cli": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "tsx src/index.ts generate --seed \"#769CDF\""
  },
  "keywords": ["material-design", "theme", "color", "cli"],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0",
  "dependencies": {
    "@material/material-color-utilities": "^0.4.0",
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
